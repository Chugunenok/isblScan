                <Key word = "SystemDialogs" />
                <Key word = "Tasks" />
                <Key word = "Wizards" />
            </KeyWords>
            
            <KeyWords name = "VariableRouteBlockCommon" bold="true" italic="false" color="Black">
                <Key word = "Work" />
                <Key word = "Sender" />
            </KeyWords>            
            
            <!--
            <KeyWords name = "VariableRouteBlockJob" bold="true" italic="false" color="Black">
                <Key word = "Job" />
                <Key word = "CreatedJobs" />
            </KeyWords>  
                
